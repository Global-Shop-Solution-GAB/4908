Program.Sub.cmdSaveBrand_Click.Start
v.Local.sUNCPath.Declare
v.Local.sMKEY.Declare
v.Local.sMVALUE.Declare
v.Local.sSql.Declare
v.Local.sFilter.Declare
v.Local.bExists.Declare
gui.FHCMaint.cmdSaveBrand.Enabled(False)
v.Local.sMKEY.Set(v.Screen.FHCMaint!txtBKey.Text)
v.Local.sUNCPath.set(v.Screen.FHCMaint!txtLogoPath.text)
v.Local.sMVALUE.set(v.Screen.FHCMaint!txtBrand.text)


f.Intrinsic.Control.If(v.Local.sMKEY.Trim,=,"","OR",v.Local.sUNCPath.trim,=,"",v.Local.sMVALUE.trim,=,"")
	f.Intrinsic.UI.Msgbox("Please make sure that the Key, Brand and the File path of the logo has been entered before saving.")
	gui.FHCMaint.cmdSaveBrand.Enabled(True)
	f.Intrinsic.Control.ExitSub
	
f.Intrinsic.Control.EndIf

f.Intrinsic.File.Exists(v.Local.sUNCPath,v.Local.bExists)
f.Intrinsic.Control.If(v.Local.bExists,=,False)
	f.Intrinsic.UI.Msgbox("You have entered an invalid logo file path. Please enter a valid file path before saving.")
	gui.FHCMaint.cmdSaveBrand.Enabled(True)
	f.Intrinsic.Control.ExitSub
f.Intrinsic.Control.EndIf
f.Intrinsic.String.Build("CAT_TYPE = 'B' and HORC = 'H' and MKEY = '{0}' and MVALUE = '{1}'",v.Local.sMKEY,v.Local.sMVALUE,v.Local.sFilter)
f.Data.Dataview.Create("MarkDT","CheckVw",22,v.Local.sFilter,"")
f.Intrinsic.Control.If(v.DataView.MarkDT!CheckVw.RowCount,>,0)
	f.Intrinsic.UI.Msgbox("There is an existing association for the pair in the database.")
	f.Data.DataView.Close("MarkDT","CheckVw")
	gui.FHCMaint.cmdSaveBrand.Enabled(True)
	f.Intrinsic.Control.ExitSub
f.Intrinsic.Control.EndIf

f.Intrinsic.String.Build("CAT_TYPE = 'B' and HORC = 'H' and MKEY = '{0}'",v.Local.sMKEY,v.Local.sFilter)
f.Data.Dataview.SetFilter("MarkDT","CheckVw",v.Local.sFilter)
f.Intrinsic.Control.If(v.DataView.MarkDT!CheckVw.RowCount,>,0)
	f.Intrinsic.UI.Msgbox("The key exists in the database")
	f.Data.DataView.Close("MarkDT","CheckVw")
	gui.FHCMaint.cmdSaveBrand.Enabled(True)
	f.Intrinsic.Control.ExitSub
f.Intrinsic.Control.EndIf


f.Intrinsic.String.Build("CAT_TYPE = 'B' and HORC = 'H' and MVALUE = '{0}'",v.Local.sMVALUE,v.Local.sFilter)
f.Data.Dataview.SetFilter("MarkDT","CheckVw",v.Local.sFilter)
f.Intrinsic.Control.If(v.DataView.MarkDT!CheckVw.RowCount,>,0)
	f.Intrinsic.UI.Msgbox("The Logo exists in the database")
	f.Data.DataView.Close("MarkDT","CheckVw")
	gui.FHCMaint.cmdSaveBrand.Enabled(True)
	f.Intrinsic.Control.ExitSub
f.Intrinsic.Control.EndIf

f.Data.DataView.Close("MarkDT","CheckVw")

f.Intrinsic.Control.Try
	f.Intrinsic.String.Build("Insert Into GCG_4908_KEYVAL Values('B','{0}','{1}','{2}','H')",v.Local.sMKEY,v.Local.sMVALUE,v.Local.sUNCPath,v.Local.sSql)
	f.ODBC.Connection!conx.Execute(v.Local.sSql)
	f.Data.DataTable.AddRow("MarkDT","CAT_TYPE","B","MKEY",v.Local.sMKEY,"MVALUE",v.Local.sMVALUE,"UNC_PATH",v.Local.sUNCPath,"HORC","H")
	gui.FHCMaint..ClearGroup(4)
	gui.FHCMaint.cmdSaveBrand.Enabled(True)
f.Intrinsic.Control.Catch
	f.Intrinsic.UI.Msgbox("Oops! Something went wrong. You may try again or contact Global Shop Solutions for assistance.")
	gui.FHCMaint.cmdSaveBrand.Enabled(True)
f.Intrinsic.Control.EndTry
Program.Sub.cmdSaveBrand_Click.End

