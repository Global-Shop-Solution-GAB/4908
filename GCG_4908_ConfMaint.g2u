F.Intrinsic.Control.If(V.Local.sRet,=,"***CANCEL***")
	f.Intrinsic.Control.ExitSub
f.Intrinsic.Control.else
	f.Intrinsic.String.Split(v.Local.sRet,"*!*",v.local.sRet)
	gui.FHCMaint.txtOVRCustomer.Text(v.Local.sRet(0).Trim)	
	gui.FHCMaint.txtOVRCustomerName.Text(v.Local.sRet(1).Trim)
f.Intrinsic.Control.EndIf
Program.Sub.cmdOVRCustBrowser_Click.End

Program.Sub.cmdOVRSave_Click.Start
v.Local.sClass.Declare
v.Local.sDiameter.Declare
v.Local.sAssy.Declare
v.Local.sKey.Declare
v.Local.sSvcTest.Declare
v.Local.sTrdName.Declare
v.Local.sSql.Declare
v.Local.sFilter.Declare
V.Local.sCustomer.Declare(String)

v.Local.sClass.Set(v.screen.FHCMaint!ddlOVRClass.Value)
v.Local.sDiameter.Set(v.screen.FHCMaint!ddlOVRDiam.Value)
v.Local.sAssy.Set(v.screen.FHCMaint!ddlOVRAssembly.Value)

f.Intrinsic.Control.If(v.Local.sClass.Trim,=,"","OR",v.Local.sDiameter.trim,=,"",v.Local.sAssy.Trim,=,"")
	f.Intrinsic.UI.Msgbox("Please make sure that the Class, Diameter and the assembly data has been entered before saving.")
	f.Intrinsic.Control.ExitSub
f.Intrinsic.Control.EndIf

f.Intrinsic.String.build("{0}{1}{2}",v.Local.sClass.Trim,v.Local.sDiameter.Trim,v.Local.sAssy.Trim,v.Local.sKey)

v.Local.sSvcTest.Set(v.screen.FHCMaint!txtOVRServiceTest.Text)
v.Local.sTrdName.Set(v.screen.FHCMaint!txtOVRTradeName.Text)

f.Intrinsic.Control.If(v.Local.sSvcTest.Trim,=,"","OR",v.Local.sTrdName.trim,=,"")
	f.Intrinsic.UI.Msgbox("Please make sure that the Service Test and Trade Name data has been entered before saving.")
	f.Intrinsic.Control.ExitSub
f.Intrinsic.Control.EndIf

V.Local.sCustomer.Set(V.Screen.FHCMaint!txtOVRCustomer.Text.Trim)
F.Intrinsic.Control.If(V.Local.sCustomer.Trim, =, "")
	f.Intrinsic.UI.Msgbox("Please make sure that the Customer has been entered before saving.")
	f.Intrinsic.Control.ExitSub
F.Intrinsic.Control.EndIf

f.Intrinsic.String.Build("MSVCKEY = '{0}' and Customer = '{1}'",v.Local.sKey, V.Local.sCustomer.Trim, v.Local.sFilter)
f.Data.Dataview.Create("dtOverride","CheckVw",22,v.Local.sFilter,"")
f.Intrinsic.Control.If(v.DataView.dtOverride!CheckVw.RowCount,>,0)
	f.Intrinsic.UI.Msgbox("There is an existing association for the Customer, Class, diameter and Assy in the database.")
	f.Data.DataView.Close("dtOverride","CheckVw")
	f.Intrinsic.Control.ExitSub

f.Intrinsic.Control.EndIf
f.Data.DataView.Close("dtOverride","CheckVw")

f.Intrinsic.Control.Try
	f.Intrinsic.String.Build("Insert Into GCG_4908_SVCTST Values('{0}','{1}','{2}','{3}','{4}','{5}', '{6}')",v.Local.sClass,v.Local.sDiameter.trim,v.Local.sAssy,v.Local.sKey,v.Local.sSvcTest,v.Local.sTrdName, V.Local.sCustomer.Trim, v.Local.sSql)
	f.ODBC.Connection!conx.Execute(v.Local.sSql)
	f.Data.DataTable.AddRow("dtOverride","MCLASS",v.Local.sClass,"MDIAM",v.Local.sDiameter.trim,"MASSY",v.Local.sAssy,"MSVCKEY",v.Local.sKey,"SVC_TST",v.Local.sSvcTest,"TRD_NAME",v.Local.sTrdName, "Customer", V.Local.sCustomer.Trim, "Customer_Name", V.Screen.FHCMaint!txtOVRCustomerName.Text.Trim)
	GUI.FHCMaint.txtOVRCustomer.Text("")
	GUI.FHCMaint.txtOVRCustomerName.Text("")
	GUI.FHCMaint.txtOVRServiceTest.Text("")
	GUI.FHCMaint.txtOVRTradeName.Text("")
	GUI.FHCMaint.ddlOVRAssembly.ListIndex(-1)
	GUI.FHCMaint.ddlOVRClass.ListIndex(-1)
	GUI.FHCMaint.ddlOVRDiam.ListIndex(-1)
f.Intrinsic.Control.Catch
	f.Intrinsic.UI.Msgbox("Oops! Something went wrong. You may try again or contact Global Shop Solutions for assistance.")
f.Intrinsic.Control.EndTry
Program.Sub.cmdOVRSave_Click.End

