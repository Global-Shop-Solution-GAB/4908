Program.Sub.cmdSaveHB_Click.Start
v.Local.sSQL.Declare
v.Local.sClass.Declare
v.Local.sDiam.Declare
v.Local.sHB.Declare
v.Local.sFilter.Declare

gui.FHCMaint.cmdSaveHB.Enabled(False)
v.Local.sClass.Set(v.Screen.FHCMaint!ddlAClass.text)
v.Local.sDiam.Set(v.Screen.FHCMaint!ddlDiameter.text)
v.Local.sHB.Set(v.Screen.FHCMaint!ddlHoseBowl.text)
gui.FHCMaint.cmdSaveHB.Enabled(False)
f.Intrinsic.Control.If(v.Local.sClass.Trim,=,"","OR",v.Local.sDiam.Trim,=,"",v.Local.sHB.Trim,=,"")
	f.Intrinsic.UI.Msgbox("Please make sure that the Class, Diameter and Hose Bowl drop downs have a valid selection before saving.")
	gui.FHCMaint.cmdSaveHB.Enabled(True)
	f.Intrinsic.Control.ExitSub

f.Intrinsic.Control.EndIf
f.Intrinsic.String.Build("MCLASS = '{0}' AND MDIAM = '{1}' AND MHB = '{2}'",V.Local.sClass,v.Local.sDiam,v.Local.sHB,v.Local.sFilter)
f.Data.DataView.Create("CDHBDT","TmpVw",22,v.Local.sFilter,"")

f.Intrinsic.Control.If(v.DataView.CDHBDt!TmpVw.RowCount,>,0)
	f.Intrinsic.UI.Msgbox("The association currently exists in the database.")
	gui.FHCMaint.cmdSaveHB.Enabled(True)
	f.Intrinsic.Control.ExitSub
	
f.Intrinsic.Control.EndIf
f.Data.DataView.Close("CDHBDT","TmpVw")


f.Intrinsic.Control.Try

	
	f.Intrinsic.String.Build("Insert Into GCG_4908_CDHB Values('{0}','{1}','{2}')",v.Local.sClass,v.Local.sDiam,v.Local.sHB,v.Local.sSql)
	f.ODBC.Connection!conx.Execute(v.Local.sSql)
	f.Data.DataTable.AddRow("CDHBDT","MCLASS",v.Local.sClass,"MDIAM",v.Local.sDiam,"MHB",v.Local.sHB)
	gui.FHCMaint..ClearGroup(9)
	gui.FHCMaint.cmdSaveHB.Enabled(True)
f.Intrinsic.Control.Catch
	f.Intrinsic.UI.Msgbox("Oops! Something went wrong. You may try again or contact Global Shop Solutions for assistance.")
	gui.FHCMaint.cmdSaveHB.Enabled(True)
f.Intrinsic.Control.EndTry



Program.Sub.cmdSaveHB_Click.End

Program.Sub.GsGCCDHB_RowCellClick.Start
v.Local.sSQL.Declare
v.Local.sClass.Declare
v.Local.sFilter.Declare
v.Local.sDiam.Declare
v.Local.sHB.Declare
v.Local.iRet.Declare
v.Local.iCount.Declare
gui.FHCMaint.GsGCCDHB.enabled(False)
v.Local.sClass.Set(v.DataTable.CDHBDT(v.Args.rowindex).MCLASS!FieldVal)
v.Local.sDiam.Set(v.DataTable.CDHBDT(v.Args.rowindex).MDIAM!FieldVal)
v.Local.sHB.Set(v.DataTable.CDHBDT(v.Args.rowindex).MHB!FieldVal)


f.Intrinsic.Control.If(v.Args.Column.UCase,=,"DELETE")
	f.Intrinsic.UI.Msgbox("Are you sure you want to delete this association? Doing so will delete all corresponding part association data as well","Delete Association?",4,v.Local.iRet)
	f.Intrinsic.control.If(v.Local.iRet,=, 7)
		gui.FHCMaint.GsGCCDHB.enabled(True)
		f.Intrinsic.Control.ExitSub
	f.Intrinsic.Control.EndIf
	f.Intrinsic.String.Build("Delete from GCG_4908_CDHB where MCLASS = '{0}' and MDIAM = '{1}' and MHB = '{2}'",v.Local.sClass,v.Local.sDiam,v.Local.sHB,v.Local.sSql)
	f.ODBC.Connection!conx.Execute(v.Local.sSql)
	f.Intrinsic.String.Build("Delete from GCG_4908_CDHBPART where MCLASS = '{0}' and MDIAM = '{1}' and MHB = '{2}'",v.Local.sClass,v.Local.sDiam,v.Local.sHB,v.Local.sSql)
	f.ODBC.Connection!conx.Execute(v.Local.sSql)
	f.Data.DataTable.DeleteRow("CDHBDT",v.Args.rowindex)
	gui.FHCMaint.GsGCCDHB.enabled(True)
	f.Intrinsic.Control.ExitSub
f.Intrinsic.Control.ElseIf(v.Args.Column.UCase,=,"MCLASS","OR",v.Args.Column.UCase,=,"MDIAM")
	gui.FHCMaint..Enabled(False)
	gui.FHCMaint.GsGCCDHBPart.SuspendLayout
	f.Data.Datatable.DeleteRow("CHDBPartDT")
	f.Data.Dictionary.CreateFromSQL("CHDBPartDict","Conx","Select MCLASS + MDIAM + MHB + PART,Enabled from GCG_4908_CDHBPART")
	f.Data.Dictionary.SetDefaultReturn("CHDBPartDict",False)
	f.Intrinsic.String.Build("Select Distinct MHB from GCG_4908_CDHB where MCLASS = '{0}' and MDIAM = '{1}'",v.Local.sClass,v.Local.sDiam,v.Local.sSQL)
	f.ODBC.Connection!conx.OpenLocalRecordSetRO("RstHB",v.Local.sSQL)
	F.Intrinsic.Control.DoUntil(V.ODBC.Conx!RstHB.EOF,=,True)
		f.Intrinsic.String.Build("MHB = '{0}'",v.ODBC.conx!RstHB.FieldVal!MHB,v.Local.sFilter)
		f.Data.DataView.Create("INVDT","TempVw",22,v.Local.sFilter,"")
		f.Data.DataView.ToDataTable("INVDT","TempVw","TempInvDT")
		f.Data.DataView.Close("INVDT","TempVw")
		f.Data.DataTable.SetValue("TempInvDT",-1,"MCLASS",v.Local.sClass)
		f.Data.DataTable.SetValue("TempInvDT",-1,"MDIAM",v.Local.sDiam)
		f.Data.DataTable.AddExpressionColumn("TempInvDT","CDHBPartAsoc","String","MCLASS + MDIAM + MHB + PART")
		f.Data.DataTable.Merge("TempInvDT","CHDBPartDT",true,2)
		f.Data.DataTable.Close("TempInvDT")
		F.ODBC.Conx!RstHB.MoveNext
	F.Intrinsic.Control.Loop
	f.ODBC.conx!RstHB.Close
	f.Data.DataTable.FillFromDictionary("CHDBPartDT","CHDBPartDict","CDHBPartAsoc","Enabled")

	f.Data.Dictionary.Close("CHDBPartDict")	
	gui.FHCMaint.GsGCCDHBPart.ResumeLayout	
	gui.FHCMaint.GsGCCDHB.enabled(True)
	gui.FHCMaint..Enabled(True)
f.Intrinsic.Control.ElseIf(v.Args.Column.UCase,=,"MHB")
	gui.FHCMaint..Enabled(False)
	gui.FHCMaint.GsGCCDHBPart.SuspendLayout
	f.Data.Datatable.DeleteRow("CHDBPartDT")
	f.Data.Dictionary.CreateFromSQL("CHDBPartDict","Conx","Select MCLASS + MDIAM + MHB + PART,Enabled from GCG_4908_CDHBPART")
	f.Data.Dictionary.SetDefaultReturn("CHDBPartDict",False)

	f.Intrinsic.String.Build("MHB = '{0}'",v.Local.sHB,v.Local.sFilter)
	f.Data.DataView.Create("INVDT","TempVw",22,v.Local.sFilter,"")
	f.Data.DataView.ToDataTable("INVDT","TempVw","TempInvDT")
	f.Data.DataView.Close("INVDT","TempVw")
	f.Data.DataTable.SetValue("TempInvDT",-1,"MCLASS",v.Local.sClass)
	f.Data.DataTable.SetValue("TempInvDT",-1,"MDIAM",v.Local.sDiam)
	f.Data.DataTable.AddExpressionColumn("TempInvDT","CDHBPartAsoc","String","MCLASS + MDIAM + MHB + PART")
	f.Data.DataTable.Merge("TempInvDT","CHDBPartDT",true,2)
	f.Data.DataTable.Close("TempInvDT")
	f.Data.DataTable.FillFromDictionary("CHDBPartDT","CHDBPartDict","CDHBPartAsoc","Enabled")
	f.Data.Dictionary.Close("CHDBPartDict")
	gui.FHCMaint.GsGCCDHBPart.ResumeLayout	
	gui.FHCMaint.GsGCCDHB.enabled(True)
	gui.FHCMaint..Enabled(True)

f.Intrinsic.Control.EndIf
gui.FHCMaint.GsGCCDHB.enabled(True)
Program.Sub.GsGCCDHB_RowCellClick.End

